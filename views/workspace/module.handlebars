<h1>Modules</h1>

<div class="modules">
        {{#each modules}}
        <div class="module">


            <div class="details">
           
                <p>ID: {{this.moduleId}}</p>
                <p>Name: {{this.moduleName}}</p>
                <p>Desc: {{this.moduleDescription}}</p>
                <p>Date: {{this.moduleDate}}</p>
            </div>



         
            
                <a href="/workspace/{{../sectionID}}/modules/attendance">Attendance</a>
            
        </div>


        {{/each}}
    </div>
